{
  "manifest_version": 2,
  "name": "Google Forms Hack",
  "version": "1.0",
  "description": "Automate Google Forms interactions",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
  },
  "browser_action": {
    "default_title": "Google Forms Hack",
    "default_popup": "popup.html"
  }
}

